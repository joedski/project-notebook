{
  "name": "project-notebook",
  "version": "0.0.0",
  "description": "Project Notebook",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:*",
    "dev:client": "run-p dev:client:*",
    "dev:client:build": "webpack --watch --config build/webpack.dev.config.js",
    "dev:client:serve": "http-server -p 8000 dist",
    "dev:server": "nodemon --watch src/node_modules/@@server/ src/node_modules/@@server/api",
    "test": "run-s test:*",
    "test:client": "echo \"Error: no test:client specified\" && exit 1",
    "test:server": "echo \"Error: no test:server specified\" && exit 1",
    "knex": "knex",
    "nodemon": "nodemon",
    "psql": "docker-compose exec db psql project_book postgres",
    "webpack": "webpack"
  },
  "keywords": [
    "project",
    "organization",
    "task-management",
    "research",
    "experiment"
  ],
  "author": "Joesph Sikorski <joedski@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "flyd": "0.2.8",
    "hyperhtml": "2.23.0",
    "knex": "0.16.3",
    "koa": "2.6.2",
    "koa-body": "4.0.6",
    "koa-router": "7.4.0",
    "pg": "7.7.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.0.1",
    "http-server": "0.11.1",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "style-loader": "0.23.1",
    "webpack": "4.28.1",
    "webpack-cli": "3.1.2"
  }
}
